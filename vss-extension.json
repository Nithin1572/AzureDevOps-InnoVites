{
    "manifestVersion": 1,
    "id": "AzureDevOps-InnoVites",
    "publisher": "Nithin",
    "version": "1.0.0",
    "name": "AzureDevOps-InnoVites",
    "description": "A simple Azure DevOps extension for InnoVites",
    "public": false,
    "categories": ["Azure Repos"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "BP-Extension",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "BP Extension",
                "uri": "index.html"
            }
        }
    ],
    "files": [
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}